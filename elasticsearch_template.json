{
  "order": 0,
  "index_patterns": "pg-*",
  "settings": {
    "index": {
      "mapping": {
        "total_fields": {
          "limit": "1000"
        }
      },
      "refresh_interval": "1s"
    }
  },
  "mappings": {
    "pglog": {
      "_meta": {
        "version": "5.6.2"
      },
      "date_detection": false,
      "dynamic_templates": [
        {
          "strings_as_keyword": {
            "mapping": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "match_mapping_type": "string"
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "application_name": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "avg_duration_ms": {
          "type": "float"
        },
        "code_module": {
          "type": "keyword"
        },
        "code_repo": {
          "type": "keyword"
        },
        "code_source": {
          "type": "nested"
        },
        "code_tags": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "command_tag": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "database_name": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "detail": {
          "norms": false,
          "type": "text"
        },
        "error_severity": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "message": {
          "norms": false,
          "type": "text"
        },
        "mvc_action": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "mvc_application": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "mvc_code_line": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "mvc_controller": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "prepared": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "prepared_step": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "process_id": {
          "type": "integer"
        },
        "notes": {
          "norms": false,
          "type": "text"
        },
        "query": {
          "norms": false,
          "type": "text"
        },
        "redis_key": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "remote_host": {
          "type": "keyword"
        },
        "session_id": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "session_line_num": {
          "type": "integer"
        },
        "session_start_time": {
          "type": "date"
        },
        "temp_table_size": {
          "type": "long"
        },
        "total_count": {
          "type": "long"
        },
        "total_duration_ms": {
          "type": "float"
        },
        "unique_sha": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "unique_string": {
          "norms": false,
          "type": "text",
          "fields": {
            "raw": {
              "type": "keyword"
            }
          }
        },
        "unknown_message": {
          "norms": false,
          "type": "text"
        },
        "user_name": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "virtual_transaction_id": {
          "ignore_above": 1024,
          "type": "keyword"
        }
      }
    }
  },
  "aliases": {}
}