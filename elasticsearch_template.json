{
  "index_patterns": [
    "pg-*"
  ],
  "order": 0,
  "template": {
    "mappings": {
      "aliases": {
        "mydata": {}
      },
      "pglog": {
        "_meta": {
          "version": "5.6.2"
        },
        "properties": {
          "created_at": {
            "format": "EEE MMM dd HH:mm:ss Z yyyy",
            "type": "date"
          },
          "host_name": {
            "type": "keyword"
          }
        }
      }
    },
    "settings": {
      "index": {
        "mapping": {
          "total_fields": {
            "limit": "1000"
          }
        },
        "refresh_interval": "1s"
      }
    }
  },
  "version": 3
}